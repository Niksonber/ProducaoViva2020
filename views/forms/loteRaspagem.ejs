<!DOCTYPE html>
<html>
  <head>
    <title>Formulário</title>
    <% include ../partials/head %>
  </head>
  <body>
    <% include ../partials/sidebar_top %>

    <!-- Raspagem -->
    <h3>Processo de raspagem</h3>
    <form method="POST" >
        <div class="form-group row">
            <div class="col-sm-10">
            <select name="LoteImpressaoId" class="custom-select" id="Lote_impressao_ID">
              <option selected="true" disabled>Lote de Impressão</option>
              <% loteImpressao.forEach(m => { %>
                <option value="<%= m.id %>"><%= m.id %></option>
              <% }); %>
            </select>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
            <input type="number" step="1" name='qtd_falhas' min="1" class="form-control col" placeholder="Quantidade de falhas" required>
            </div>
        </div>
        <button type="submit" class="btn btn-success" >Cadastrar</button>
    </form>



  <% include ../partials/sidebar_bottom %>
  </body>
  <% include ../partials/bottom %>
</html>